  <div class="page-header">

    <h1><%= user.local.name || user.twitter.name %> </h1>

  </div>

  <button type="button" id="delete-user">DELETE USER</button>

  <div>
    <div>
      <% if(user.local.name){ %>
        <h3>Locally Stored User</h3>

        <p>
          <strong>Name</strong>: <%= user.local.name %><br>
          <strong>id</strong>: <%= user._id %><br>
          <strong>email</strong>: <%= user.local.email %><br>
          <strong>password</strong>: <%= user.local.password %>
        </p>

      <% } else { %>
        <h3>Twitter User </h3>

        <p>
          <strong>Name</strong>: <%= user.twitter.name %><br>
          <strong>id</strong>: <%= user.twitter.id %><br>
          <strong>username</strong>: <%= user.twitter.username %><br>
          <strong>token</strong>: <%= user.twitter.token %>
        </p>

      <% } %>

    </div>

  </div>

  <script type="text/javascript">

  var $deleteUser = $('#delete-user')

  $deleteUser.on('click', function(){
    console.log('<%=user._id%>')
    $
      .ajax({
        method: 'delete',
        url: 'profile/<%= user._id %>',
        success: function(result){
          window.location.replace('/')
        }
      })
  })




  </script>
